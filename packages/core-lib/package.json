{
  "name": "@cloudforet/core-lib",
  "version": "1.12.0-dev11",
  "description": "Cloudforet Console Core Library",
  "scripts": {
    "build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "dev": "rimraf dist && tsc-watch --project . --outDir ./dist --onSuccess \"tsc-alias -w\"",
    "lint": "eslint --ext .js,.ts src",
    "format": "eslint --fix --ext .js,.ts src",
    "test": "vitest run",
    "test:watch": "vitest watch --ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spaceone-dev/console-core-lib.git"
  },
  "author": "Cloudforet",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^0.25.0",
    "axios-auth-refresh": "^3.2.2",
    "bytes": "^3.1.2",
    "dayjs": "^1.10.7",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "luxon": "^1.28.1"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.1",
    "@types/luxon": "^1.27.1",
    "@types/node": "^16.11.43",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint-config-custom": "*",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.6.11",
    "tsc-watch": "^6.0.0",
    "tsconfig": "*",
    "vite": "^4.3.9"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    ">=3.1": {
      "*": [
        "./dist/*"
      ]
    }
  }
}
