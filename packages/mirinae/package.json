    "marked": "^4.0.12",
    "uuid": "^8.3.2",
    "v-click-outside": "^3.0.1",
    "v-tooltip": "^2.0.3",
    "velocity-animate": "^1.5.2",
    "vue": "^3.3.4",
    "vue-filepond": "^6.0.3",
    "vue-focus": "^2.1.0",
    "vue-fragment": "^1.5.1",
    "vue-gtag": "^1.16.1",
    "vue-i18n": "^9.2.2",
    "vue-js-toggle-button": "^1.3.3",
    "vuedraggable": "^2.24.3",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
