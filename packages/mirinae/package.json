    "resolve-url-loader": "^3.1.0",
    "rollup-plugin-copy": "^3.4.0",
    "semantic-release": "^19.0.3",
    "stylelint": "^13.2.1",
    "stylelint-config-standard": "^20.0.0",
    "tailwindcss": "^1.9.6",
    "ts-loader": "^7.0.4",
    "tsc-alias": "^1.3.7",
    "tsc-watch": "^6.0.0",
    "tsconfig": "*",
    "typescript": "^4.8.2",
    "vite": "^3.2.4",
    "vite-plugin-stylelint": "^3.0.9",
    "vitest": "^0.26.1",
    "vue-tsc": "^1.0.9"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 9"
  ],
  "engines": {
    "node": ">= 16.13.2",
    "npm": ">= 8.1.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
