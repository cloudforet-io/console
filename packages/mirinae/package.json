    "@kyvg/vue3-notification": "^2.9.1",
    "@popperjs/core": "^2.11.4",
    "@vue/compat": "^3.3.4",
    "@vueuse/components": "^9.0.2",
    "@vueuse/core": "^8.9.1",
    "@yzfe/svgicon": "^1.1.0",
    "@yzfe/vue3-svgicon": "^1.0.1",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "ajv-i18n": "^4.2.0",
    "bytes": "^3.1.2",
    "codemirror": "^5.65.6",
    "color": "^3.1.2",
    "commander": "^10.0.1",
    "core-js": "^3.29.1",
    "dayjs": "^1.10.7",
    "dompurify": "^2.3.3",
    "dragselect": "^1.15.1",
    "ejs": "^3.1.9",
    "filepond": "^4.30.3",
    "filepond-plugin-file-validate-type": "^1.2.6",
    "flatpickr": "^4.6.9",
    "floating-vue": "^2.0.0-beta.20",
